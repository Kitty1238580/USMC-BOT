{
  "name": "boommodbot",
  "version": "1.0.0",
  "description": "Discord bot app",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start-dis": "node ./nodeBot/index.js",
