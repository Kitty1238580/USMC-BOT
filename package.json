    "ffmpeg-static": "^5.2.0",
    "itty-router": "^4.0.27",
    "jsdom": "^24.0.0",
    "libsodium-wrappers": "^0.7.13",
    "prism-media": "^1.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^2.0.15",
    "prettier": "^3.2.5",
